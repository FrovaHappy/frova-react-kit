{
  "name": "frova-react-kit",
  "version": "0.0.1",
  "description": "React components for frova",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FrovaHappy/frova-react-kit"
  },
  "bugs": {
    "url": "https://github.com/FrovaHappy/frova-react-kit/issues"
  },
  "type": "module",
  "files": [
    "dist/**/*"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "build": "rollup -c --bundleConfigAsCjs",
    "dev": "tsup --watch --onSuccess \"npm link\"",
    "publish": "tsup && npm publish --access public"
  },
  "keywords": [],
  "author": "frova_happy",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@testing-library/react": "^15.0.7",
    "@types/module-alias": "^2.0.4",
    "@types/remark-heading-id": "^1.0.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "@vitest/coverage-v8": "^1.6.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "globals": "^15.2.0",
    "jsdom": "^24.0.0",
    "module-alias": "^2.2.3",
    "postcss": "^8.4.38",
    "postcss-modules": "^6.0.0",
    "postcss-preset-env": "^9.5.14",
    "rollup": "^4.18.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.77.2",
    "string-hash": "^1.1.3",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.9.0",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "rehype-highlight": "^7.0.0",
    "rehype-stringify": "^10.0.0",
    "remark": "^15.0.1",
    "remark-heading-id": "^1.0.1",
